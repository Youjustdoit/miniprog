<!--miniprogram/pages/aboutme/aboutme.wxml-->
<view class='container'>
  <view class="userinfo">
    <block>
      <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" mode="cover"></image>
      <text class="userinfo-nickname">{{userInfo.nickName}}</text>
    </block>
  </view>
</view>
<view class="weui-cells weui-cells_after-title">
  <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <image src="{{icon1}}" style="margin-right: 20px;vertical-align: middle;width:30px; height: 30px;"></image>
    </view>
    <view class="weui-cell__bd">收藏</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>

  <navigator url="../col/col" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <image src="{{icon2}}" style="margin-right: 20px;vertical-align: middle;width:30px; height: 30px;"></image>
    </view>
    <view class="weui-cell__bd">报考院校</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>

  <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <image src="{{icon3}}" style="margin-right: 20px;vertical-align: middle;width:30px; height: 30px;"></image>
    </view>
    <view class="weui-cell__bd">设置</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>

  <navigator url="./about/about" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <image src="{{icon4}}" style="margin-right: 20px;vertical-align: middle;width:30px; height: 30px;"></image>
    </view>
    <view class="weui-cell__bd">关于我们</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>

</view>